        <div id="main">

            <div id="content" class="instapaper_body" role="main">
                <section id="content-body">
                    <div id="topic">
                        @foreach (var item in Model.Contents)
                        {
                            <h1 id="title" data-id="5303827">


                                <a href="/HomePage/AddContent/" itemprop="url">
                                    <span itemprop="name">@item.Heading.HeadingName</span>
                                </a>

                            </h1>

                            <ul id="entry-item-list" class="home-page-entry-list" style="margin-bottom:50px;">

