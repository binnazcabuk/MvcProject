@using Entity.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda Veri Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Hakkımda</th>

        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>
    @foreach (var item in Model)
    {
        

        <tr>
            <th>@item.AboutID</th>
            <td>@item.AboutDetails1</td>
         
            <td><a href="#" class="btn btn-warning">aktif</a></td>
            <td><a href="#" class="btn btn-warning">pasif</a></td>
        </tr>
    }
</table>

@Html.Action("AboutPartial","About")
